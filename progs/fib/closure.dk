# Itterative closure to generate fibonacci sequence

proc fibFn {} {
	var {first = 1; second = 1}

	return {} => {
		var newSecond = first + second
		var result = first

		first = second
		second = newSecond

		next result
	}
}


proc printFibs {count} {
	var ff = [fibFn]

	for n in (1 .. count) {
		say [ff] " "
	}
	puts
}

printFibs [str2num (Sys::Argv@0)]



