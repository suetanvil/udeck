
puts "Getting docstrings..."
var keys = [_::docstring_keys]

puts "Sorting keys..."
keys = [sort {cmp a b} keys]

puts "=head1 Functions\n\n"
puts "=over\n\n"

var functionKeys = [reduce {value ([_::docstring_get a]@0 == :proc)} keys]

for fk in functionKeys {
	var doc = [_::docstring_get fk]
	puts "=item $fk\n"


}

