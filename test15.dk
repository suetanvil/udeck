
proc cmp {a b} {
	if {==  a b} {
		puts a " == " b
	} else {
		puts a " != " b
	}
}


cmp 1 1
cmp 2 1
cmp "foo" 4

cmp "foo" "bar"
cmp "foo" "foo"

var f
set f "foo"
cmp f f

cmp "foo" :foo
cmp :foo "foo"
cmp :foo :foo

cmp :foo :bar


cmp [list 1 2 3] [list 4 5 6]
cmp [list 1 2 3 "foo"] [list 1 2 3 "foo"]
cmp [list 1 2 3 "foo"] [list 1 2 3 "foo" 5 6]

