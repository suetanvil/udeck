# Test access to individual characters in a string

puts ("abcde"@1)

var a = "abcde"
puts (a@0) (a@4) (a@ -1) (a@ -5)

a = :abcde
puts (a@0) (a@4) (a@ -1) (a@ -5)

a = "abcde"
a@4 = 'x'
puts a

a@0 = '@'
puts a


var x = [byteArray 33 34 35 36 37]
var n = 0

while (n < [size x]) {
	puts "Value at " n " = " (x@n)
	n = n + 1
}

