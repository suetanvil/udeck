
# Test non-arg sub modifier.
mproc foo {
	sub 1 fn
	list l
} {
	puts "calling " fn " on " l
	fn l
}


foo {puts a} [+ 2 2]
foo {puts a} [a b 'hello' 41.9 [xxx]]
foo {puts a} :[1 2 3]
foo {puts a} (total = sum*filter + total)
foo {puts a} :(total = sum*filter + total)

var x = 42
foo {puts a} x
foo {puts a} [val x]

