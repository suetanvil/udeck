# Tests for methods of built-in classes.

class Foo {
	var a b c
	method bar {n} {puts n}
}

puts Foo.name
Foo.name = "bar"
puts Foo.name
puts Foo.selectors
puts Foo.class.selectors

puts " --- "
puts Foo.isNil
puts nil.isNil


puts " --- "
puts 42.isTrue
puts 0.isTrue

puts "hello, world".isTrue
puts "".isTrue
puts nil.isTrue

puts :[].isTrue
puts :[1 2 3].isTrue
puts " --- "


puts 42.isIndexable
puts nil.isIndexable
puts :[1 2 3].isIndexable
puts "foo".isIndexable
puts [byteArray 1 2 3 4 5].isIndexable
puts :Foo.isIndexable
puts " --- "

