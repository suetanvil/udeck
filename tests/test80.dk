# Test OO arithmetic

# Modulo:

var {a = 5; b = 3.0; c = 2}
puts " --- %"
puts (a%3)
puts (a%b)
puts (4%b)

# multiplication
puts " --- *"
puts (a*b)
puts (b*a)
puts (2*c)
puts (c*4)
puts (a*0)
puts (0*a)

# division
puts " ---  /"
puts (a/b)
puts (b/a)
puts (c/2)

# truncated division
puts " --- //"
puts (a // b)
puts (b // a)
puts (c // 2)

# less than
puts " --- <"
puts (a < b)
puts (b < a)
puts (a < a)
puts (a < 5)

# power
puts " --- **"
puts (2**a)
puts (c**a)
puts (4**4)

# Bitwise &
puts " --- &"
puts (b&c)
puts (b&b)
puts (c&1)

# Bitwise |
puts " --- |"
puts (b|c)
puts (b|b)
puts (a|b)
puts (a|0)

# bitwise ^
puts " --- ^"
puts (a^a)
puts (b^b)
puts (b^c)

# subtract and negate
puts " --- -"
puts [- a]
puts (b - a)
puts (a - b)




