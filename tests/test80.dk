# Test OO arithmetic

# Modulo:

var {a = 5; b = 3.0; c = 2}
puts " --- %"
puts (a%3)
puts (a%b)
puts (4%b)

# multiplication
puts " --- *"
puts (a*b)
puts (b*a)
puts (2*c)
puts (c*4)
puts (a*0)
puts (0*a)

# division
puts " ---  /"
puts (a/b)
puts (b/a)
puts (c/2)

# truncated division
puts " --- //"
puts (a // b)
puts (b // a)
puts (c // 2)
