# Also test misc. string attributes
for i in :["a" "e" "i" "o" "u" "y" "x" "w" "z"
		   "A" "E" "I" "O" "U" ""
		   "Aasd" "Efes" "ieos" "Ousi" "Uisn"] \
	{
		var is = ""

		if (i.isVowel) {
			is = "is"
		} else {
			is = "is not"
		}
		puts "$i $is a vowel"
	}
